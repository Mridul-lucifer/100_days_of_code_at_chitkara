import java.util.Scanner;

public class table {
    public static void main(String args[]) {
        int a;
        System.out.println("enter the number");
        Scanner sc = new Scanner(System.in);
        a = sc.nextInt();
        for (int j = 1; j < 11; j++) {
            System.out.println(j + " X " + a + " = " + (j * a));
        }
    }
}
