import java.util.Scanner;

public class Numerical_pattern {
    public static void main(String Args[]) {
        int a;
        System.out.print("Enter the Size : ");
        Scanner sc = new Scanner(System.in);
        a = sc.nextInt();
        for (int i = 1; i <= a; i++) {
            for (int j = 1; j <= i; j++) {
                System.out.print(j + " ");
            }
            System.out.println();
        }
    }
}
