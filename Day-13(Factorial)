import java.util.Scanner;

public class Factorial {
    public static void main(String Args[]) {
        System.out.print("Enter the number : ");
        Scanner sc = new Scanner(System.in);
        int a = sc.nextInt();
        int fact = 1;
        for (int i = a; i > 0; i--) {
            fact = fact * i;
        }
        System.out.println("The factorial is = " + fact);
    }

}
