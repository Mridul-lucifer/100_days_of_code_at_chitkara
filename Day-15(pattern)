import java.util.Scanner;

public class Pattern {
    public static void main(String Args[]) {
        int a;
        System.out.println("enter the size of pattern ");
        Scanner sc = new Scanner(System.in);
        a = sc.nextInt();
        for (int i = 1; i <= a; i++) {
            for (int j = 1; j <= i; j++) {
                System.out.print(" " + j);
            }
            System.out.println("");
        }
    }
}
